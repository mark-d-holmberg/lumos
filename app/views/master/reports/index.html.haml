- title "Reports"

.col-lg-4
  .well.dashboard-box
    %h3.text-center.dashboard-box-title
      = link_to pending_impressions_reports_path do
        %span.glyphicon.glyphicon-map-marker
        Pending Impressions
      .dashboard-box-content
        Lists all pending impressions.

.col-lg-4
  .well.dashboard-box
    %h3.text-center.dashboard-box-title
      = link_to campaign_requests_path do
        %span.glyphicon.glyphicon-fire
        Campaign Requests
      .dashboard-box-content
        Shows requested campaigns.
